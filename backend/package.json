{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "basic node.js, typescript, and jest project starter",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/server.ts --watch src",
    "lint": "prettier --write \"{src,test}/**/*.ts\""
  },
  "keywords": [
    "typescript",
    "nodejs",
    "starter",
    "jest"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "hnswlib-node": "^1.4.2",
    "jest": "^24.9.0",
    "langchain": "^0.0.202",
    "nodemon": "^3.0.2",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "type": "module"
}
